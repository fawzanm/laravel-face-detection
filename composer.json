{
  "name": "freearhey/laravel-face-detection",
  "description": "A Laravel Package for Face Detection and Cropping in Images.",
  "license": "MIT",
  "keywords": [
    "Laravel",
    "Face Detection"
  ],
  "authors": [
    {
      "name": "Arhey"
    }
  ],
  "require": {
    "php": "^7.3| ^8.0 | ^8.1",
    "illuminate/support": "^8.0",
    "intervention/image": "^2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Arhey\\FaceDetection\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Arhey\\FaceDetection\\Tests\\": "tests"
    }
  },
  "minimum-stability": "stable"
}
